{
  "name": "strex-lang",
  "version": "0.1.0",
  "description": "The *StrexLang* (\"Strange expression language\") is used to train regular expressions for JavaScript in a hopefully not so boring way.",
  "bin": "src/cli.js",
  "private": true,
  "scripts": {
    "start": "node src/cli.js",
    "test": "jest",
    "lint": "eslint '**/*.js'",
    "commit": "git-cz",
    "release": "standard-version"
  },
  "keywords": [
    "strexlang",
    "language",
    "regex",
    "test",
    "training"
  ],
  "author": "Martin Helmut Fieber <info@martin-fieber.de> (https://martin-fieber.de)",
  "license": "MIT",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "commitizen": "2.10.1",
    "cz-conventional-changelog": "2.1.0",
    "eslint": "5.4.0",
    "eslint-config-google": "0.9.1",
    "jest": "^23.5.0",
    "standard-version": "4.4.0"
  }
}
