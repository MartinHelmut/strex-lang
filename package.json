{
  "name": "strex-lang",
  "version": "0.1.0",
  "description": "The *StrexLang* (\"Strange expression language\") is used to train regular expressions in JavaScript. Also a bit of parser design in a hopefully not so boring way.",
  "bin": "src/cli/index.js",
  "private": true,
  "scripts": {
    "start": "node src/cli",
    "test": "node src/cli --test",
    "lint": "eslint '**/*.js'",
    "release": "standard-version",
    "format": "prettier --write \"**/*.{js,css,md,json,yml}\"",
    "format:check": "prettier --list-different \"**/*.{js,css,md,json,yml}\""
  },
  "keywords": [
    "strexlang",
    "language",
    "regex",
    "test",
    "training"
  ],
  "author": "Martin Helmut Fieber <info@martin-fieber.de> (https://martin-fieber.de)",
  "license": "MIT",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "devDependencies": {
    "commander": "3.0.0",
    "eslint": "6.2.2",
    "eslint-config-google": "0.13.0",
    "eslint-config-prettier": "6.1.0",
    "eslint-plugin-prettier": "3.1.0",
    "jest": "24.9.0",
    "pegjs": "0.10.0",
    "prettier": "1.18.2",
    "standard-version": "7.0.0"
  },
  "dependencies": {
    "decimal.js": "10.2.0"
  }
}
